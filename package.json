{
  "name": "synyster",
  "version": "1.0.0",
  "description": "A simple blog",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:scss && next build",
    "build:scss": "node-sass styles/main.scss static/build/main_${HEROKU_SLUG_COMMIT:-$(git rev-parse HEAD)}.css",
    "build-scss:watch": "node-sass -w styles/ -o static/dist/",
    "dev": "cross-env NODE_ENV=devolopment nodemon --watch ./server/src ./server/src/server.js & npm run build-scss:watch",
    "start": "cross-env NODE_ENV=production node ./server/src/server.js",
    "start-prod": "cross-env NODE_ENV=production forever ./server/src/server.js",
    "analyze": "cross-env ANALYZE=1 next build",
    "prestart": "rm -fr ./build & npm run build:scss & next build",
    "deploy-heroku": "git push heroku master"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vthang95/synyster"
  },
  "keywords": [
    "[\"react\"",
    "[\"redux\"",
    "\"next\"",
    "\"next.js\"",
    "\"nextjs\"",
    "\"ant",
    "design\"",
    "\"boilerplate\"",
    "\"example\"]"
  ],
  "author": "vthang95",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vthang95/synyster/issues"
  },
  "homepage": "https://github.com/vthang95/synyster#readme",
  "dependencies": {
    "antd": "^3.2.0",
    "async": "^2.6.0",
    "axios": "^0.17.1",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.3.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "highlight.js": "^9.12.0",
    "immutability-helper": "^2.5.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.12",
    "moment": "^2.19.1",
    "mongoose": "^5.0.4",
    "morgan": "^1.9.0",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "node-sass": "^4.7.2",
    "nprogress": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "speakingurl": "^14.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "glob": "^7.1.2",
    "nodemon": "^1.15.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.0.8",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "styled-jsx-postcss": "^0.2.0",
    "webpack-bundle-analyzer": "^2.9.1"
  }
}
